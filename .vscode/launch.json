{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Next.js",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [],
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        "**/.next/**",
        "**/next/dist/**"
      ],
      "smartStep": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
